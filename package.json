{
  "name": "aframe-presentation-component",
  "description": "AFrame Presentation component",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && cp index.js dist/aframe-presentation-component.js && ./node_modules/.bin/webpack-cli --entry ./index.js --output dist/aframe-presentation-component.min.js --mode production",
    "dev": "./node_modules/.bin/budo index.js:aframe-presentation-component.min.js --dir examples --port 7000 --live",
    "start": "npm run dev"
  },
  "keywords": [
    "aframe",
    "aframe-example",
    "aframe-boilerplate",
    "aframe-scene",
    "presentation",
    "slidedeck",
    "webvr",
    "vr"
  ],
  "dependencies": {
    "aframe": "^0.9.2",
    "aframe-environment-component": "^2.0.0",
    "aframe-look-at-component": "^0.8.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10"
  },
  "devDependencies": {
    "budo": "^11.2.0"
  }
}
